extends layout


block content
  .row.banner
    .col-12
      h1= pageHeader.title
        small &nbsp;#{pageHeader.strapLine}

  .row
    .col-12.col-md-8
      each post in posts
        .card
          .card-block
            h4
              a(href=`/post/${post.post_id}`) #{post.title}
              span.badge.badge-pill.badge-default.float-right
                  i(class="icon-calendar")
                    = moment(post.time_stamp).format(" DD MMMM YYYY, dddd")
            p= post.about
            
            


